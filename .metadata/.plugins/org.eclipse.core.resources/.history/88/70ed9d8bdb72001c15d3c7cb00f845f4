package com.java8;

public class MethodAndConstructor {  
    public static void findcube(){  
        System.out.println("Cube of Number : " + ()); 
    }  
    public static void main(String[] args) {  
        // Referring static method  
        MethodRefServices m = MethodAndConstructor::findcube;
        m.cube();
    }  
}  
